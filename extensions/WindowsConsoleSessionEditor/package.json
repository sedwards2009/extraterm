{
  "name": "windows-console-session-editor",
  "description": "Windows Console Session Editor",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/WindowsConsoleSessionEditorExtension.js",
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\""
  },
  "dependencies": {
    "extraterm-trim-between-tags": "1.0.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.13",
    "vue": "2.6.10",
    "vue-class-component": "6.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^12.6.2",
    "eslint": "6.8.0",
    "eslint-config-extraterm": "1.0.0",
    "@extraterm/extraterm-extension-api": "0.1.0",
    "shx": "^0.3.2",
    "typescript": "3.7.4"
  },
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionEditors": [
      {
        "name": "Windows Console",
        "type": "windows-console",
        "css": {
          "fontAwesome": true
        }
      }
    ]
  }
}
