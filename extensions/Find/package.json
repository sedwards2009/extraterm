{
  "name": "find",
  "description": "Find in text",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/FindExtension.js",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "extraterm-trim-between-tags": "1.0.0",
    "vue": "2.6.10",
    "vue-class-component": "6.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "extraterm-extension-api": "0.1.0",
    "typescript": "3.7.4"
  },
  "contributes": {
    "commands": [
      {
        "command": "find:find",
        "title": "Find",
        "category": "terminal",
        "icon": "fas fa-search",
        "when": "terminalFocus"
      }
    ],
    "terminalBorderWidgets": [
      {
        "name": "find",
        "css": {
          "fontAwesome": true
        }
      }
    ],
    "menus": {
      "contextMenu": [
        {
          "command": "find:find",
          "show": true
        }
      ]
    }
  }
}
