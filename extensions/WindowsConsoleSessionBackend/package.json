{
  "name": "windows-console-session-backend",
  "description": "Windows Console Session Backend",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/WindowsConsoleSessionBackendExtension.js",
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\""
  },
  "dependencies": {
    "extraterm-event-emitter": "1.0.0",
    "lodash": "^4.17.19",
    "node-pty": "0.10.0-beta2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/node": "^12.6.2",
    "eslint": "6.8.0",
    "eslint-config-extraterm": "1.0.0",
    "@extraterm/extraterm-extension-api": "0.5.0",
    "shx": "^0.3.2",
    "typescript": "3.9.6"
  },
  "isInternal": true,
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionBackends": [
      {
        "name": "Windows Console",
        "type": "windows-console"
      }
    ]
  }
}
