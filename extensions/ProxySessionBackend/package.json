{
  "name": "proxy-session-backend",
  "description": "Proxy based Session Backend",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/ProxySessionBackendExtension.js",
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc && shx cp -r src/python dist/python && shx cp resources/extraterm-wsl-proxy dist/",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\""
  },
  "dependencies": {
    "extraterm-event-emitter": "1.0.0",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/node": "^12.6.2",
    "eslint": "7.19.0",
    "eslint-config-extraterm": "1.0.0",
    "@extraterm/extraterm-extension-api": "0.13.0",
    "shx": "^0.3.2",
    "typescript": "4.1.3"
  },
  "isInternal": true,
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionBackends": [
      {
        "name": "Cygwin",
        "type": "cygwin"
      },
      {
        "name": "Windows Subsystem for Linux",
        "type": "wsl"
      }
    ]
  }
}
