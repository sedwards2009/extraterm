{
  "name": "proxy-session-backend",
  "description": "Proxy based Session Backend",
  "version": "1.0.0",
  "main": "src/ProxySessionBackendExtension.js",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "extraterm-event-emitter": "1.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/node": "7.0.5",
    "extraterm-extension-api": "0.1.0",
    "typescript": "3.1.6"
  },
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionBackends": [
      {
        "name": "Cygwin",
        "type": "cygwin"
      },
      {
        "name": "Windows Subsystem for Linux",
        "type": "wsl"
      }
    ]
  }
}
