{
  "name": "proxy-session-editor",
  "description": "Proxy based Session Editor",
  "version": "1.0.0",
  "main": "src/ProxySessionEditorExtension.js",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "extraterm-trim-between-tags": "1.0.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.11",
    "vue": "2.6.10",
    "vue-class-component": "6.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^10.12.18",
    "extraterm-extension-api": "0.1.0",
    "typescript": "3.4.5"
  },
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionEditors": [
      {
        "name": "Cygwin",
        "type": "cygwin",
        "css": {
          "fontAwesome": true
        }
      },
      {
        "name": "Windows Subsystem for Linux",
        "type": "wsl",
        "css": {
          "fontAwesome": true
        }
      }
    ]
  }
}
