{
  "name": "unix-session-backend",
  "description": "Unix Session Backend",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "exports": "./dist/UnixSessionBackendExtension.js",
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\""
  },
  "dependencies": {
    "extraterm-event-emitter": "1.0.0",
    "extraterm-later": "1.0.0",
    "lodash-es": "4.17.21",
    "node-pty": "0.10.0-beta19"
  },
  "devDependencies": {
    "@extraterm/extraterm-extension-api": "0.15.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^16.4.0",
    "eslint": "8.13.0",
    "eslint-config-extraterm": "1.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "shx": "^0.3.2",
    "typescript": "4.7.4"
  },
  "excludePlatform": [
    {
      "os": "win32"
    }
  ],
  "isInternal": true,
  "contributes": {
    "sessionBackends": [
      {
        "name": "Unix",
        "type": "unix"
      }
    ]
  }
}
