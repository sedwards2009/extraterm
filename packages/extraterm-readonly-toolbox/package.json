{
  "name": "extraterm-readonly-toolbox",
  "version": "0.1.0",
  "description": "TypeScript based support for readonly types",
  "author": "Simon Edwards",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/sedwards2009/extraterm.git"
  },
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\"",
    "run": "false",
    "test": "yarn run build && yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "devDependencies": {
    "eslint": "8.13.0",
    "eslint-config-extraterm": "1.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "jest": "27.5.1",
    "shx": "^0.3.2",
    "typescript": "4.6.3"
  }
}
